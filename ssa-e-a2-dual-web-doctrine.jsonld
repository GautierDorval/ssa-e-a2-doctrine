        },
        {
          "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#layers-q-layer-md"
        }
      ]
    },

    {
      "@type": "DefinedTerm",
      "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#ssa-e-r",
      "name": "SSA-E-R",
      "alternateName": [
        "Restitution proportionality module",
        "Restitution proportionnée (RFC)"
      ],
      "termCode": "SSA-E-R",
      "description": "SSA-E-R is an RFC doctrinal module applied after Q-Layer authorization. It governs restitution only: the depth and form of an already-legitimate answer (canonical, structured, contextual, analytical) without altering response authorization outcomes. It must not be interpreted as granting additional inference rights and must fall back to Q-Layer outputs when compliant restitution would require invention.",
      "inDefinedTermSet": {
        "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#defined-term-set"
      },
      "creator": {
        "@id": "https://gautierdorval.com/#gautier-dorval"
      },
