        }
      ]
    },
    {
      "@type": "DefinedTerm",
      "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#a2",
      "name": "A2",
      "alternateName": [
        "Amplification interpretative ciblée"
      ],
      "termCode": "A2",
      "description": "A2 is a constrained interpretive amplification layer within the SSA-E + Dual Web doctrine. It defines non-transactional interpretation rules, scope boundaries, explicit exclusions, and canonical references for high-risk pages where misinterpretation is likely.",
      "inDefinedTermSet": {
        "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#defined-term-set"
      },
      "creator": {
        "@id": "https://gautierdorval.com/#gautier-dorval"
      },
      "isPartOf": {
        "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#ssa-e-a2-dual-web-doctrine"
      },
      "isReferencedBy": [
        {
          "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#doctrine-md"
        },
