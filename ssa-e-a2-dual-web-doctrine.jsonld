        },
        {
          "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#terminology-md"
        }
      ]
    },
    {
      "@type": "DefinedTerm",
      "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#q-layer",
      "name": "Q-Layer",
      "alternateName": [
        "Interpretative legitimacy layer",
        "Response authorization layer"
      ],
      "termCode": "QLayer",
      "description": "Q-Layer is a transversal interpretative legitimacy layer activated between understanding (SSA-E + Dual Web verification) and amplification (A2). It governs the conditions required to produce an answer, including source sufficiency, scope validity, disambiguation sufficiency, and non-transactional constraints. When legitimacy conditions are not satisfied, legitimate non-response (authoritative silence) or targeted clarification is required.",
      "inDefinedTermSet": {
        "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#defined-term-set"
      },
      "creator": {
        "@id": "https://gautierdorval.com/#gautier-dorval"
      },
      "isPartOf": {
        "@id": "https://github.com/GautierDorval/ssa-e-a2-doctrine#ssa-e-a2-dual-web-doctrine"
      },
